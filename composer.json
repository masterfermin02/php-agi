{
  "name": "fperdomo/php-agi",
  "description": "Modern PHP 8.3+ fork of PHPAGI for building Asterisk AGI, FastAGI, and AMI integrations.",
  "keywords": ["phpagi", "asterisk", "agi", "voip", "fastagi", "ami", "ivr", "callcenter", "telephony"],
  "homepage": "https://fperdomo.dev",
  "license": "GNU",
  "version": "1.0.1",
  "authors": [
    {
      "name": "Fermin Perdomo",
      "homepage": "https://fperdomo.dev",
      "role": "Developer"
    }
  ],
  "support": {
    "email": "masterfermin02@gmail.com"
  },
  "require": {
    "php" : ">=8.3",
    "ext-sockets": "*"
  },
  "autoload": {
    "psr-4": {
      "Fperdomo\\PhpAgi\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "lint": "pint",
    "refactor": "./vendor/bin/rector",
    "test:lint": "pint --test",
    "test:unit": "pest --parallel",
    "test:refactor": "rector --dry-run",
    "test": [
      "@test:unit",
      "@test:lint",
      "@test:refactor"
    ]
  },
  "require-dev": {
    "pestphp/pest": "^4.1",
    "rector/rector": "^2.3",
    "laravel/pint": "^1.26"
  },
  "config": {
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
